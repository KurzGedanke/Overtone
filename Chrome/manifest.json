{
  "manifest_version": 3,
  "name": "Overtone",
  "version": "1.0",
  "author": "KurzGedanke",
  "description": "Adds volume controll to overcast.fm",
  "homepage_url": "https://github.com/KurzGedanke/Overtone",
  "icons": {
    "16": "/icons/icon16.png",
    "32": "/icons/icon32.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "content_scripts": [{
    "matches": ["*://*.overcast.fm/*"],
    "js": ["src/overtone.js"]
  }]
}